{
  "expo": {
    "name": "Pick & Go",
    "slug": "pickngo",
    "owner": "kushenthimira",
    "version": "0.1.0-beta",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.cipher.pickngo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cipher.pickngo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
